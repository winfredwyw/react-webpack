{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "基于webpack的react开发环境",
  "main": "index.js",
  "scripts": {
    "predev": "npm install supervisor -g",
    "dev": "cross-env NODE_ENV=dev supervisor -w ./server -- ./server -h 0.0.0.0",
    "prebuild": "gulp copy",
    "build": "cross-env NODE_ENV=production webpack --color --progress --config ./bin/webpack.config.js",
    "postbuild": "gulp better",
    "build-show": "cross-env NODE_ENV=production webpack --profile --json > stats.json --config ./bin/webpack.config.js",
    "debug": "cross-env NODE_ENV=debug webpack -w -d --color --progress --config ./bin/webpack.config.js",
    "clean": "gulp clean"
  },
  "keywords": [
    "react",
    "webpack",
    "es6"
  ],
  "author": "winfred",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "colors": "^1.1.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "ejs": "^2.5.6",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-smushit": "^1.2.0",
    "node-sass": "^4.5.2",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hammerjs": "^0.5.0",
    "react-loader": "^2.4.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reload": "^1.1.7",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "http-proxy-middleware": "^0.17.4"
  }
}
