{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "基于webpack的react开发环境",
  "main": "index.js",
  "scripts": {
    "predev-server": "cross-env NODE_ENV=dev webpack --progress --config ./bin/webpack.config.js",
    "dev-server": "cross-env NODE_ENV=dev node ./server --host 0.0.0.0",
    "prebuild-server": "cross-env NODE_ENV=pro webpack --color --progress --config ./bin/webpack.config.js",
    "build-server": "cross-env NODE_ENV=dev node ./server --host 0.0.0.0",
    "build": "cross-env NODE_ENV=pro webpack --color --progress --config ./bin/webpack.config.js"
  },
  "keywords": [
    "react",
    "webpack",
    "es6"
  ],
  "author": "winfred",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "colors": "^1.1.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-loader": "^2.4.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}